Proceso calcular_area_y_volumen_cilindro
	Escribir "ingrese radio"
	Leer radio;
	Escribir "ingrese altura"
	Leer altura;
	areacilindro<-2*3.14*radio*altura;
	volumencilindro<-3.14*areacilindro*areacilindro*altura;
	Escribir "el area del cilindro:",areacilindro;
	Escribir "el volumen del cilindro:",volumencilindro;
FinProceso
