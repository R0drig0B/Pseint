Proceso promedio_suma
	Escribir "ingrese a"
	Leer a;
	Escribir "ingrese b"
	Leer b;
	Escribir "ingrese c"
	Leer c;
	promedio<-a*b*c/3;
	suma<-a+b+c;
	dobleprimernumero<-2*a;
	triplesugundonumero<-3*b;
	cuadrupletercernumero<-4*c;
	Escribir "promedio:",promedio;
	Escribir "suma:",suma;
	Escribir "dobre primer numero:",dobleprimernumero;
	Escribir "trile segundo numero:",triplesugundonumero;
	Escribir "cuadruple tercer numero:", cuadrupletercernumero;
	
FinProceso
